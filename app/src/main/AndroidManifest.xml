<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Gym Guide"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".WorkoutListActivity"></activity>
        <activity
            android:name=".SingleExerciseActivity"
            android:label="@string/title_activity_single_exercise_activty"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".QRCodeActivity" />
        <activity android:name=".ResetActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>